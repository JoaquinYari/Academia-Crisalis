public class Client {
	
	public List<Pedido> pedidos;
	public List<Service> serviciosActivos;
	
	public Client () {
		pedidos = new ArrayList<Pedido>();
		serviciosActivos = new ArrayList<Service>();
	}
	
	
		
}
