package modelo.cliente;

import java.time.LocalDate;

public class Empresa extends Cliente {
	
	private String razonSocial;
	private Integer cuit;
	private String inicioActividad;
	private Persona responsable;
	
	public Empresa(String razonSocial, Integer cuit, String inicioActividad, String apellido, String nombre, Integer dni) {
		super();
		this.razonSocial = razonSocial;
		this.cuit = cuit;
		this.inicioActividad = inicioActividad;
		responsable = new Persona(apellido, nombre, dni);
	}

	@Override
	public String toString() {
		return "Empresa [razonSocial=" + razonSocial + ", cuit=" + cuit + ", inicioActividad=" + inicioActividad
				+ ", responsable=" + responsable + "]";
	}

	
	
	
}
